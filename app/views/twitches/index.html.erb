<p id="notice"><%= notice %></p>

<h1>Twitches</h1>

<table>
  <thead>
    <tr>
      <th>Owner</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twitches.each do |twitch| %>
      <tr>
        <td><%= twitch.owner %></td>
        <td><%= twitch.url %></td>
        <td><%= link_to 'Show', twitch %></td>
        <td><%= link_to 'Edit', edit_twitch_path(twitch) %></td>
        <td><%= link_to 'Destroy', twitch, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twitch', new_twitch_path %>
